{
  "name": "dawg-unscramble",
  "version": "1.0.0",
  "description": "UnScramble given letters to create real words",
  "main": "index.js",
  "scripts": {
    "test": "echo 'run someday: mocha'",
    "eslint": "eslint  --ext .js src/js/UnScramble.js src/js/BinFileReader.js",
    "clint": "lint  unscramble.c",
    "lint": "npm run eslint && npm run clint",
    "start": "web-dev-server --node-resolve --open --watch",
    "devServer": "webpack serve  ",
    "devServerPack": "webpack-dev-server --inline --color --openPage ",
    "prebuild": "echo 'Not running clean'",
    "prebuild:dev": "echo 'Not running clean'",
    "prebuild:prod": "npm run clean",
    "build": "npm run build:dev",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev": "npm run build:dev",
    "postbuild": "echo 'npm run types'",
    "watch": "webpack --mode development --watch",
    "startBuild": "npm run watch",
    "predist": "npm run build:prod && npm run test",
    "t": "web-dev-server",
    "try": "web-dev-server --node-resolve --open --app-index UnScramble.html",
    "trym": "web-dev-server --open \"Chrome\" --app-index UnScramble.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ztalbot2000/DAWG-UnScramble.git"
  },
  "keywords": [
    "UnScramble",
    "Scramble",
    "letters",
    "letter",
    "Word",
    "Words",
    "Wordle",
    "Dictionary"
  ],
  "author": "John Talbot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ztalbot2000/DAWG-UnScramble/issues"
  },
  "homepage": "https://github.com/ztalbot2000/DAWG-UnScramble#readme",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
